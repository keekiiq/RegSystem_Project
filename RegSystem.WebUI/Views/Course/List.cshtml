@using RegSystem.Domain.Entities
@model IEnumerable<Course>
@{
    ViewBag.Title = "Courses";
}
@foreach (var p in Model)
@model RegSystem.WebUI.Models.ProductsListViewModel
@{
    ViewBag.Title = "Courses";
}
@foreach (var p in Model.Courses)
{
<div class="well">
    <h3>
        <strong>@p.Name</strong>
        <span class="pull-right label label-primary">@p.Section</span>
    </h3>
    <span class="lead"> @p.Description</span>
</div>
}

<div class="btn-group pull-right">
    @Html.PageLinks(Model.PagingInfo, x => Url.Action("List", new { page = x }))
</div>
{
<div>
    <h1>
        @p.CourseID
    </h1>
</div>
}
